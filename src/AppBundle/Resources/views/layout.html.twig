<!DOCTYPE html>
<html lang="{{ locale|e }}">
<head>
    {% spaceless %}
        <meta charset="UTF-8">
        <meta http-equiv="X-UA-Compatible" content="IE=Edge">
        <title>{% block title %}Home{% endblock %}</title>
        <meta name="author" content="ООО ИнфоСистем">
        <meta name="description" content="{% block meta_description %}{% endblock %}">
        <meta name="keywords" content="{% block meta_keywords %}{% endblock %}">
        {% block stylesheets %}
            {% stylesheets 'bundles/app/css/bootstrap.min.css'
                'bundles/app/css/bootstrap-theme.min.css'
                filter = 'cssrewrite' %}<link rel="stylesheet" href="{{ asset_url }}">
            {% endstylesheets %}
        {% endblock %}
        {% block javascripts %}
            {% javascripts '@AppBundle/Resources/public/js/jquery-1.11.3.min.js'
                '@AppBundle/Resources/public/js/bootstrap.min.js' %}
                <script src="{{ asset_url }}"></script>
            {% endjavascripts %}
        {% endblock %}
        <!--[if lt IE 9]>
            {% javascripts '@AppBundle/Resources/public/js/html5shiv.js' %}
                <script src="{{ asset_url }}"></script>
            {% endjavascripts %}
        <![endif]-->
    {% endspaceless %}
    {% block head %}{% endblock %}
</head>
<body>
    {% block body %}{% endblock %}
</body>
</html>